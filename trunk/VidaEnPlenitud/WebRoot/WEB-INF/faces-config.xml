		<display-name>seguimientoBancoExpertos</display-name>
		<from-view-id>/seguimientoBancoExpertos.iface</from-view-id>
		<navigation-case>
			<from-outcome>ConsultoriaPDF</from-outcome>
			<to-view-id>/pdf/consultoria.iface</to-view-id>
